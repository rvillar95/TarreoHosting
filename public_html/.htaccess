RewriteRule ^(.*)$ https://www.tarreoinformatica2019.com/$1 [L,R=301]
RewriteCond %{HTTPS} off
RewriteCond %{HTTP:X-Forwarded-SSL} !on
RewriteCond %{HTTP_HOST} ^tarreoinformatica2019\.com$ [OR]
